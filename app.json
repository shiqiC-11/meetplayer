{
  "pages": [
    "pages/index/index",
    "pages/publish/publish",
    "pages/slot-detail/slot-detail",
    "pages/venue-detail/venue-detail",
    "pages/venue-list/venue-list",
    "pages/my-slots/my-slots",
    "pages/my/my",
    "pages/login/login"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#52C41A",
    "navigationBarTitleText": "MeetPlayer",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#F5F5F5"
  },
  "tabBar": {
    "color": "#8C8C8C",
    "selectedColor": "#52C41A",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "white",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "附近",
        "iconPath": "assets/icons/nearby.jpg",
        "selectedIconPath": "assets/icons/nearby.jpg"
      },
      {
        "pagePath": "pages/my-slots/my-slots",
        "text": "我的约球",
        "iconPath": "assets/icons/slots.jpg",
        "selectedIconPath": "assets/icons/slots.jpg"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "assets/icons/profile.jpg",
        "selectedIconPath": "assets/icons/profile.jpg"
      }
    ]
  },
  "usingComponents": {
    "van-button": "@vant/weapp/button/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-field": "@vant/weapp/field/index",
    "van-toast": "@vant/weapp/toast/index",
    "van-dialog": "@vant/weapp/dialog/index",
    "van-loading": "@vant/weapp/loading/index",
    "van-picker": "@vant/weapp/picker/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-tab": "@vant/weapp/tab/index",
    "van-tabs": "@vant/weapp/tabs/index",
    "van-tag": "@vant/weapp/tag/index",
    "van-icon": "@vant/weapp/icon/index",
    "van-image": "@vant/weapp/image/index",
    "van-card": "@vant/weapp/card/index",
    "van-stepper": "@vant/weapp/stepper/index",
    "van-checkbox": "@vant/weapp/checkbox/index",
    "van-checkbox-group": "@vant/weapp/checkbox-group/index",
    "van-radio": "@vant/weapp/radio/index",
    "van-radio-group": "@vant/weapp/radio-group/index",
    "van-rate": "@vant/weapp/rate/index",
    "van-slider": "@vant/weapp/slider/index",
    "van-uploader": "@vant/weapp/uploader/index",
    "van-datetime-picker": "@vant/weapp/datetime-picker/index",
    "van-search": "@vant/weapp/search/index",
    "van-empty": "@vant/weapp/empty/index",
    "van-divider": "@vant/weapp/divider/index"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于显示附近的约球局"
    }
  },
  "requiredPrivateInfos": [
    "getLocation",
    "chooseLocation"
  ],
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  }
}
